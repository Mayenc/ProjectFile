<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProjectFile</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ProjectFile.styles.css" asp-append-version="true" />

    <link rel="stylesheet" href="~/lib/jqueryui/themes/base/theme.css"/>
    <link rel="stylesheet" href="~/lib/jqueryui/themes/base/jquery-ui.css"/>
    <link rel="stylesheet" href="~/lib/elfinder/css/elfinder.full.css"/>
    <link rel="stylesheet" href="~/lib/elfinder/css/theme.min.css">

    <script rel="stylesheet" href="~/lib/jquery/dist/jquery.min.js"></script>
    <script rel="stylesheet" href="~/lib/jqueryui/jquery-ui.min.js"></script>
    <script rel="stylesheet" href="~/lib/elfinder/js/elfinder.min.js"></script>
</head>
<body>
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
